//programme to print fibonacci sequence 2

#include<stdio.h>

int fibonacci(int n);

int main(){
    int n;
    printf("Kaca kadar?:");
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        printf("%d ",fibonacci(i));
    }
    return 0;
}

int fibonacci(int n){
    if(n<=1){
        return n;
    }
    else {
        return fibonacci(n-1)+fibonacci(n-2);
    }
}
