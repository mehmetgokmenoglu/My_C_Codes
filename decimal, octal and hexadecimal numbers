//Decimal, octal and hexadecimal numbers from 1 to 15
#include <stdio.h>
int main()
{
    int i;
    for(i=0;i<=15;i++) {
        if (i == 0)
        printf("DECIMAL\n");
            printf("%d ", i);
    }
    for(i=0;i<=15;i++) {
        if (i == 0)
        printf("\nOCTAL\n");
            printf("%o ", i);
    }
    for(i=0;i<=15;i++){
            if(i==0)
        printf("\nHEXADECIMAL\n");
            printf("%X ",i);
        }
    return 0;
}
